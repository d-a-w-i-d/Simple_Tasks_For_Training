matrix = [ [4, 2, 7, 3], [11, 1, 10, 0], [23, 12, 77, 5]]
# Matrix
print "*** Matrix ***"
cells = []
for row in range(4):
    for col in range(len(matrix)):
        cells.append(matrix[col][row])
    print cells
    cells = []

# Rows
print "\n*** Rows ***"
for row in range(4):
    for col in range(len(matrix)):
        print matrix[col][row]
        
# Columns
print "\n*** Columns ***"
for lst in matrix:
    for i in range(len(lst)):
        print lst[i]
